<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.lx199.gear.MainActivity">

    <TextView
        android:id="@+id/tv1"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginTop="5dp"
        android:text="齿数" />

    <TextView
        android:id="@+id/tv2"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginTop="50dp"
        android:text="模数" />

    <TextView
        android:id="@+id/tv3"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginTop="95dp"
        android:text="压力角" />

    <EditText
        android:id="@+id/et3"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/tv3"
        android:layout_marginRight="30dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:text="20" />

    <TextView
        android:id="@+id/tv4"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:layout_marginTop="140dp"
        android:text="螺旋角" />

    <EditText
        android:id="@+id/et4"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/tv4"
        android:layout_marginRight="30dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:text="0" />

    <Button
        android:id="@+id/bt1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/et5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp"
        android:onClick="js"
        android:text="确定" />

    <TextView
        android:id="@+id/tv6"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_below="@+id/tv7"
        android:layout_marginTop="44dp"
        android:text="跨齿数" />

    <EditText
        android:id="@+id/et2"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:layout_alignTop="@+id/tv2"
        android:layout_alignLeft="@+id/et1"
        android:layout_alignStart="@+id/et1" />

    <TextView
        android:id="@+id/tv7"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_marginTop="56dp"
        android:text="长度"
        android:layout_below="@+id/bt1"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <EditText
        android:id="@+id/et7"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:layout_alignBaseline="@+id/tv6"
        android:layout_alignBottom="@+id/tv6"
        android:layout_alignLeft="@+id/et6"
        android:layout_alignStart="@+id/et6" />

    <EditText
        android:id="@+id/et6"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:layout_alignBaseline="@+id/tv7"
        android:layout_alignBottom="@+id/tv7"
        android:layout_alignRight="@+id/et5"
        android:layout_alignEnd="@+id/et5" />

    <EditText
        android:id="@+id/et5"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:text="@string/_0"
        android:layout_alignTop="@+id/tv5"
        android:layout_alignLeft="@+id/et4"
        android:layout_alignStart="@+id/et4" />

    <EditText
        android:id="@+id/et1"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="number|numberSigned|numberDecimal"
        android:layout_alignTop="@+id/tv1"
        android:layout_alignLeft="@+id/et3"
        android:layout_alignStart="@+id/et3" />

    <TextView
        android:id="@+id/tv5"
        android:layout_width="70dp"
        android:layout_height="40dp"
        android:text="变位系数"
        android:layout_below="@+id/tv4"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
